void* calloc(int amount,int of);
void* malloc(int size);
void free(void* ptr);

int* twoSum(int* nums, int numsSize, int target, int* returnSize){
    int *res = calloc((*returnSize=2), 4);

    for (int i=0; i<numsSize; i = i + 1)
    {
        int x = target - *(nums + i);
        for (int j=i+1; j<numsSize; j = j + 1)
        {
            if (x == *(nums + j))
            {
                *res = i;
                *(res + 1) = j;
                return res;
            }
        }
    }
    return res; // should be NULL but not implemented
}
int main(){
  int* nums = malloc(3 * 4);
  *(nums) = 3;
  *(nums + 1) = 2;
  *(nums + 2) = 4;

  int return_size;

  int* result = twoSum(nums, 3, 6, &return_size);
  printint(return_size);
  printint(*result);
  printint(*(result + 1));

  free(result);
  free(nums);
}
